<tr id="<%= dom_id product %>" class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600">
  <th scope="row" class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
    <%= product.id %>
  </th>
  <td class="px-6 py-4">
    <%= product.sku %>
  </td>
  <td class="px-6 py-4">
    <%= product.name %>
  </td>
  <td class="px-6 py-4">
    <%= product.stock_count %>
  </td>
  <td class="px-6 py-4">
    <%= link_to "Edit", edit_product_path(product),class:"font-medium text-blue-600 dark:text-blue-500 hover:underline", data: { turbo_frame: 'modal' } %>
  </td>
</tr>